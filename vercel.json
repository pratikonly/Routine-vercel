{
       "version": 2,
       "framework": null,
       "functions": {
         "api/*.js": {
           "maxDuration": 10
         }
       },
       "rewrites": [
         {
           "source": "/api/(.*)",
           "destination": "/api/$1"
         },
         {
           "source": "/(.*)",
           "destination": "/public/$1"
         },
         {
           "source": "/",
           "destination": "/public/index.html"
         }
       ],
       "cleanUrls": true,
       "trailingSlash": false
     }
